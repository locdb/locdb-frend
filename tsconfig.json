{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "lib": [
      "es2017",
      "dom"
    ],
    "typeRoots": [
      "../node_modules/@types"
      ],
      "types" : [ "core-js", "node" ]
  },
  "exclude" : [ "node_modules"]
}
