{
  "*": {
    "dispatch": "karma start karma.conf.js --single-run",
    "make": "npm build",
    "start": "npm start"
  },
  "src/app/*.css": {"type": "css"},
  "src/app/*.html": {"alternate": "src/app/{}.ts", "type": "html"},
  "src/app/*.spec": {"type": "spec"},
  "src/app/*.ts": {"alternate": "src/app/{}.html", "type": "ts"},
  "todo.rst": {"type": "todo"}
} 
