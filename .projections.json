{
  "*": {
    "dispatch": "export CHROME_BIN=chromium; ng test --watch false",
    "make": "ng build",
    "start": "ng serve"
  },
  "src/app/*.css": {"type": "css"},
  "src/app/*.html": {"alternate": "src/app/{}.ts", "type": "html"},
  "src/app/*.spec.ts": {"type": "spec"},
  "src/app/*.ts": {"alternate": "src/app/{}.html", "type": "ts"}
} 
